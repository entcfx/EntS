function strrev(*char string) {
    int length = 0;
    while (string != '\0') {
        length++;
        [string]++;
    };
    [string] = [string] - length;
    
    int start = 0;
    
    while (start < length) {
        [string] = [string] + start;    
        char temp = string;
        [string] = [string] - start;
        [string] = [string] + length;
        char temp2 = string;
        string = temp;
        [string] = [string] - length;

        [string] = [string] + start;
        string = temp;
        [string] = [string] - start;

        start++;
        length--;
    };
} *char string;